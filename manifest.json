{
  "manifest_version": 3,
  "name": "GPT账号切换",
  "version": "1.0",
  "description": "Manage and update '__Secure-next-auth.session-token' cookie for ChatGPT",
  "permissions": [
    "cookies",
    "storage",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://*.chatgpt.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}